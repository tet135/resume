<div>
    {{#with main}}
    <main>
        {{>title data='My education' big=true}}
        <section>
            <ul>
                {{#each educations }}
                <li>
                    {{name}}
                    {{#unless isEnd}}(in the process...){{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>
        {{>title data='My sertificates' big=false }}
        <section>
            <ul>
                {{#each sertificates}}
                <li>
                    {{name}} ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>